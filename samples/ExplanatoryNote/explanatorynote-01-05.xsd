    <xs:complexType name="tTextBlock">
        <xs:sequence>
            <xs:choice minOccurs="1" maxOccurs="unbounded">
                <xs:element name="SubTitle" type="non-empty-string" maxOccurs="unbounded"/>
                <xs:element name="Text" type="non-empty-string" maxOccurs="unbounded"/>
                <xs:element name="Image" type="tImage" maxOccurs="unbounded"/>
                <xs:element name="Table" type="tTable" maxOccurs="unbounded"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="Title" type="non-empty-string" use="optional"/>
    </xs:complexType>

    <xs:complexType name="tTable">
        <xs:sequence>
            <xs:element name="TitleRow" type="tRow" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Row" type="tRow" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tRow">
        <xs:sequence>
            <xs:element name="Cell" type="tCell" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tCell">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="align" type="tAlignTypes" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="tImage">
        <xs:sequence>
            <xs:element name="ImageData" type="xs:base64Binary" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Comment" type="non-empty-string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="Type" type="tImageTypes" use="required"/>
    </xs:complexType>

    <xs:complexType name="tModel">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Модель объекта</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Description" type="non-empty-string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Описание модели</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ModelFile" type="tModelFile" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Файл, содержащий модель объекта (части объекта)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tModelFile">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Файл модели или файл части модели</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FileName" type="tFileName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Имя файла модели</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileFormat" type="tModelFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Формат модели</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileRelativePath" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Относительный путь к файлу</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileChecksum" type="tFileChecksum" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Контрольная сумма файла модели, рассчитанная по алгоритму CRC32-IEEE</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SignFile" type="tSignFile" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Файл подписи к основному файлу модели</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="description" type="non-empty-string" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="ru">Произвольное описание файла модели</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <!-- Описание простых типов -->

    <xs:simpleType name="tSchemaVersion">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Номер версии, использованной при формировании пояснительной записки в формате xx.xx</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){2}\.([0-9]){2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string50">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 50 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="50"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string100">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 100 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="100"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string200">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 200 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="200"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string500">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 500 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="500"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="string1000">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 1000 символа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="1000"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tPositiveDecimal">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Положительное вещественное число</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="tCadastralNumber">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Кадастровый номер участка</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d+:\d+:\d+:\d+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFileName">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной до 255 символов - имя файла</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="255"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tEmail">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка - адрес электронной почты</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:pattern value="[a-zA-Zа-яА-Я0-9_.\-]{1,}[@]{1}[a-zA-Zа-яА-Я0-9_.\-]{1,}[.]{1}[a-zA-Zа-яА-Я]{2,}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFunctionsClass">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Код классификатора функционального назначения объектов капитального строительства</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}\.[0-9]{1,3})|([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3})"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFunctionsClassGroup">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Код классификатора функционального назначения группы объектов капитального строительства</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFunctionsClassOKS">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Код классификатора функционального назначения объектов капитального строительства</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}\.[0-9]{1,3}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tOrgOGRN">
        <xs:annotation>
            <xs:documentation xml:lang="ru">ОГРН организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="13"/>
            <xs:pattern value="\d{13}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tOrgINN">
        <xs:annotation>
            <xs:documentation xml:lang="ru">ИНН организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="10"/>
            <xs:pattern value="\d{10}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tOrgKPP">
        <xs:annotation>
            <xs:documentation xml:lang="ru">КПП организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="9"/>
            <xs:pattern value="\d{9}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tOGRNIP">
        <xs:annotation>
            <xs:documentation xml:lang="ru">ОГРНИП</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="15"/>
            <xs:pattern value="\d{15}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tSNILS">
        <xs:annotation>
            <xs:documentation xml:lang="ru">СНИЛС</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:pattern value="[0-9]{3}-[0-9]{3}-[0-9]{3} [0-9]{2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tNOPRIZ">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Идентификационный номер в Национальном реестре специалистов НОПРИЗ</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:pattern value="(П|ПИ|И)-\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tPostIndex">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Почтовый индекс</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="6"/>
            <xs:pattern value="\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFileFormat">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 4 символа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tFileChecksum">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной 8 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9a-fA-F]{8}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tYesNo">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Непустая текстовая строка, содержащая одно из значений "да" или "нет"</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="да">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Логическое значение "да"/"истина"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="нет">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Логическое значение "нет"/"ложь"</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tYear">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Год</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="2021"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tGUID">
        <xs:annotation>
            <xs:documentation>Уникальный идентификатор формирования заключения</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="non-empty-string">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Непустая текстовая строка</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tImageTypes">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Типы изображений</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="jpg"/>
            <xs:enumeration value="gif"/>
            <xs:enumeration value="png"/>
            <xs:enumeration value="jpeg"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tRole">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Роль участника разработки, нормоконтроля или согласования документа - части проектной документации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Разработка">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Разработка</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Нормоконтроль">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Нормоконтроль</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Согласование">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Согласование</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tAlignTypes">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Варианты выравнивания текста</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="left"/>
            <xs:enumeration value="right"/>
            <xs:enumeration value="center"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tRegionsRF">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Коды субъектов Российской Федерации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="00">
