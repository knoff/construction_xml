	<xs:simpleType name="tFileFormat">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 4 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-z0-9]+"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFileChecksum">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 8 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9a-fA-F]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="non-empty-string">
		<xs:annotation>
			<xs:documentation>Непустая текстовая строка</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(\s*[^\s]\s*)+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tYesNo">
		<xs:annotation>
			<xs:documentation>Непустая текстовая строка, содержащая одно из значений "да" или "нет"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="да">
				<xs:annotation>
					<xs:documentation>Логическое значение "да"/"истина"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="нет">
				<xs:annotation>
					<xs:documentation>Логическое значение "нет"/"ложь"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tEstimatedSum">
		<xs:annotation>
			<xs:documentation>Описание сумм при указании сметной стоимости</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(Не требуется)|(Отсутствует)|(-{0,1}\d*\.{0,1}\d*)"/>
		</xs:restriction>
	</xs:simpleType>
		<xs:simpleType name="string50">
