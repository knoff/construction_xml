		<xs:restriction base="xs:string">
		<xs:pattern value="\d+:\d+:\d+:\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFileName">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной до 255 символов - имя файла</xs:documentation>
		</xs:annotation>
		<xs:restriction base="non-empty-string">
			<xs:maxLength value="255"/>
		</xs:restriction>
